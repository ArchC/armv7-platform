#include  <iostream>
#include  <systemc.h>
#include  "ac_stats_base.H"
#include  "arm.H"



int sc_main(int ac, char *av[])
{

  //!  ISA simulator
  arm arm_proc1("arm");

#ifdef AC_DEBUG
  ac_trace("arm_proc1.trace");
#endif 

